/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    
    Not16(in = a, out = nota);
    Not16(in = b, out = notb);
    
    And16(a = a, b = notb, out = aAndNotb);
    And16(a = b, b = nota, out = bAndNota);
    
    Or16(a = aAndNotb, b = bAndNota, out = out);
}
