
/**
 * Zero16:
 * out = 1 if (in[0]==0 and in[1]==0 and ... and in[15]==0)
         0 otherwise
 */

CHIP Zero16 {
    IN in[16];
    OUT out;

    PARTS:

    Or8Way(in = in[0..7], out = low);
    Or8Way(in = in[8..15], out = high);
    Or(a = low, b = high, out = o);
    Not(in = o, out = out);

}


